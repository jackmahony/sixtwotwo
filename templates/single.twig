{% extends "base.twig" %}{% block content %}<section id="projects"><div class="row"><div class="medium-12 columns">{% if post.project_title %}<h2>{{post.project_title}}</h2>{% endif %}</div></div><div class="row"><div class="medium-12 columns">{% if post.project_information %}<p class="info">{{post.project_information}}</p>{% endif %}</div></div><div class="row flexicontent"><div class="medium-12 columns">  {% for section in post.get_field('project_content') %}    {% if section.acf_fc_layout == 'sub_header' %}<div class="project-sub-header"><h3>{{section.sub_header}}</h3></div>    {% elseif section.acf_fc_layout == 'body_text' %}<div class="project-body-text"><p>{{section.body_text}}</p></div>    {% elseif section.acf_fc_layout == 'percent_image' %}<div class="project-percent-image"><img src="{{ TimberImage(section.percent_image)|resize(300, 300) }}" /></div>    {% elseif section.acf_fc_layout == 'full_image' %}<div class="medium-8 medium-offset-2 project-full-image"><img src="{{ TimberImage(section.full_image)}}" /></div>    {% endif %}  {% endfor %}</div></div></section>{% endblock %}