{% extends "base.twig" %}{% block content %}<section class="slideInUp animated" id="projects"><div class="media"><div class="row"><div class="medium-10 medium-centered columns">{% if post.content %}  {{post.content}}{% endif %}</div></div></div><div class="textblocks"><div class="row"><div class="medium-6 medium-centered columns">{% if post.project_title %}<h2>{{post.project_title}}</h2>{% endif %}</div></div><div class="row"><div class="medium-8 medium-centered columns">  {% for section in post.get_field('project_content') %}    {% if section.acf_fc_layout == 'sub_header' %}<h3>{{section.sub_header}}</h3>    {% elseif section.acf_fc_layout == 'body_text' %}<p>{{section.body_text}}</p></div></div></div><div class="vidsound"><div class="row"><div class="medium-10 medium-centered columns">    {% elseif section.acf_fc_layout == 'vid_sound' %}      {{section.vid_sound}}    {% endif %}  {% endfor %}</div></div></div></section>{% endblock %}