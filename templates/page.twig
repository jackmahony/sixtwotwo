{% extends "base.twig" %}{% block content %}{% if post.studio_image %}<section style="background: url('{{ TimberImage(post.studio_image)|resize(2000, 0) }}')center no-repeat;" class="slideInUp animated" id="home-img"><div class="medium-middle"><div data-equalizer="text" class="row"><div data-equalizer-watch="text" class="medium-12 columns">{% if post.about_text %}<h3 class="fadeInUpBig animated">{{post.about_text}}</h3>{% endif %}</div><div class="medium-12 columns">{% if post.mission %}<p class="fadeInUpBig animated">{{post.mission}}</p>{% endif %}</div></div></div></section>{% endif %}{% if post.service_icons %}<section class="slideInUp animated" id="services"><div class="row hide-for-medium"><div class="small-12 columns"><h2>Our services:</h2><br /></div></div><div data-equalizer="icons" class="row expanded"><div data-equalizer-watch="icons" class="medium-up-3">{% for item in post.get_field('service_icons') %}<div class="column"><div id="hov-{{item.icon.id}}" style="background: url('{{ TimberImage(item.icon) }}')center no-repeat;" class="hov"><div class="small-middle"><img src="{{TimberImage(item.svg_icon)|resize(50, 50) }}" /></div></div><div id="norm-{{item.icon.id}}" class="norm"><h2>{{item.title}}</h2><p>{{item.description}}</p></div></div>{% endfor %}</div></div></section>{% endif %}{% if post.image_tim %}<section id="us"><div class="row"><div class="small-12 columns hide-for-medium">{% if post.meet_the_team_title %}<h2>{{post.meet_the_team_title}}</h2>{% endif %}</div></div><div data-equalizer="tim" data-equalize-on="medium" class="row tim"><div data-equalizer-watch="tim" class="medium-6 columns">{% if post.image_tim %}<img src="{{TimberImage(post.image_tim).src}}" />{% endif %}</div><div data-equalizer-watch="tim" class="medium-6 columns"><div class="medium-middle"><h2>Tim</h2>{% if post.tim_description %}<p>{{post.tim_description}}</p>{% endif %}</div></div></div><div data-equalizer="luwi" data-equalize-on="medium" class="row lui"><div data-equalizer-watch="luwi" class="medium-6 medium-push-6 columns">{% if post.image_lewi %}<img src="{{TimberImage(post.image_lewi).src}}" />{% endif %}</div><div data-equalizer-watch="luwi" class="medium-6 medium-pull-6 columns collapse"><div class="medium-middle"><h2>Lewis</h2>{% if post.lewi_description %}<p>{{post.lewi_description}}</p>{% endif %}</div></div></div></section>{% endif %}{% if post.show_projects %}<section class="bounceInLeft animated" id="projectshome"><div class="row"><h2>Take a look at our Projects!</h2></div><div data-equalizer="project" class="row medium-up-3">{% for project in projects %}<div data-equalizer-watch="project" class="column singleproject"><a href="{{project.link}}" class="project"><div class="project-pic">{% if project.project_image %}<img src="{{TimberImage(project.project_image)|resize(300, 200) }}" />{% endif %}</div><figcaption><div class="project-info">{% if project.company_name %}<h3>{{project.company_name}}<hr /></h3>{% endif %}{% if project.project_title %}<p>{{project.project_title}}</p>{% endif %}</div></figcaption></a></div>{% endfor %}</div></section>{% endif %}{% if post.slug == 'contact' %}<section class="slideInUp animated" id="contact-text"><div class="row"><div class="medium-12 columns">{% if post.contact_text %}<p>{{post.contact_text}}</p>{% endif %}</div></div></section><section class="slideInUp animated" id="contact-form-map"><div data-equalizer="location" data-equalize-on="medium" class="row"><div data-equalizer-watch="location" class="medium-7 columns"><h3>Send us an email</h3>{% filter shortcodes %}  [contact-form-7 id="205" title="Contact Us"]{% endfilter %}</div><div data-equalizer-watch="location" class="medium-4 medium-offset-1 columns"><iframe width="370" height="450" frameborder="0" style="border:0" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3312.211967103466!2d151.20048591521052!3d-33.88419438065198!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6b12ae26e8e082af%3A0x40ef80faa7aeeab9!2s822+George+St%2C+Chippendale+NSW+2008!5e0!3m2!1sen!2sau!4v1481512433846&key=AIzaSyD45ubf_BlbU-828R6GD5QSlYcn0i5OjsU" allowfullscreen></iframe><locations><h3>Contact Us</h3>{% for location in options.business_location %}<strong>{{location.location}}</strong>    {{location.location_address.address}}{% endfor %}<div class="contact-deets">{% if options.contact_number %}<p>Contact: {{options.contact_email}}</p>{% endif %}{% if options.contact_number %}<p>Email: {{options.contact_email}}</p>{% endif %}</div></locations></div></div></section>{% endif %}{% endblock %}{% block scripts %}  {% for item in post.get_field('service_icons') %}
<script type="text/javascript">
//<![CDATA[

$(function() {

  $("#hov-{{item.icon.id}}").hover(function(){

    $("#norm-{{item.icon.id}}").toggleClass("hover");

  });

});


//]]>
</script>
  {% endfor %}{% endblock %}