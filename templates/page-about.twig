{% extends "base.twig" %}{% block content %}{% if post.studio_image %}<section style="background: url('{{ TimberImage(post.studio_image)}}')center;" class="slideInUp animated" id="home-img"><div class="medium-middle"><div data-equalizer="text" class="row"><div data-equalizer-watch="text" class="medium-8 medium-centered columns">{% if post.about_text %}<h1 class="fadeInUpBig animated">{{post.about_text}}</h1>{% endif %}</div><div class="medium-6 medium-centered columns">{% if post.mission %}<p class="fadeInUpBig animated">{{post.mission}}</p>{% endif %}</div></div></div></section>{% endif %}{% if post.service_icons %}<section class="slideInUp animated" id="services"><div data-equalizer="icons" class="row expanded"><div data-equalizer-watch="icons" class="medium-up-3">{% for item in post.get_field('service_icons') %}<div class="column"><div id="hov-{{item.icon.id}}" style="background: url('{{ TimberImage(item.icon) }}')center no-repeat;" class="hov"><div class="small-middle"><img src="{{TimberImage(item.svg_icon)|resize(60, 60) }}" /></div></div><div id="norm-{{item.icon.id}}" class="norm"><h3>{{item.title}}</h3><p>{{item.description}}</p></div></div>{% endfor %}</div></div></section>{% endif %}{% if post.image_tim %}<section id="us"><div data-equalizer="tim" data-equalize-on="medium" class="row"><div data-equalizer-watch="tim" class="medium-6 columns"><div class="tim">{% if post.image_tim %}<img src="{{TimberImage(post.image_tim).src}}" />{% endif %}</div></div><div data-equalizer-watch="tim" class="medium-6 columns"><div class="medium-middle"><h2>Tim</h2>{% if post.tim_description %}<p>{{post.tim_description}}</p>{% endif %}</div></div></div><div data-equalizer="luwi" data-equalize-on="medium" class="row"><div data-equalizer-watch="luwi" class="medium-6 medium-push-6 columns"><div class="lui">{% if post.image_lewi %}<img src="{{TimberImage(post.image_lewi).src}}" />{% endif %}</div></div><div data-equalizer-watch="luwi" class="medium-6 medium-pull-6 columns collapse"><div class="medium-middle"><h2>Lewis</h2>{% if post.lewi_description %}<p>{{post.lewi_description}}</p>{% endif %}</div></div></div></section>{% endif %}{% endblock %}